{
    "name": "liran-co/laravel-intercom",
    "description": "Laravel wrapper for Intercom PHP.",
    "keywords": [
        "laravel",
        "intercom",
        "laravel-intercom"
    ],
    "homepage": "https://github.com/liran-co/laravel-intercom",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Liran Cohen",
            "email": "l@liran.co"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/support": "^5.6|^6.0",
        "intercom/intercom-php": "^4.1",
        "php-http/guzzle6-adapter": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LiranCo\\LaravelIntercom\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LiranCo\\LaravelIntercom\\LaravelIntercomServiceProvider"
            ],
            "aliases": {
                "Intercom": "LiranCo\\LaravelIntercom\\LaravelIntercomFacade"
            }
        }
    }
}
